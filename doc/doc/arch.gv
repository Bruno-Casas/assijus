  "assijus-site" -> assijus

  "assijus-site" -> "assijus.exe"
  "assijus.exe" -> "assijus"

  "apolo-signer" -> "pdf-converter"
  "apolo-signer" -> "apolo"

  "sigadoc-signer" -> "blucservice"
  "sigadoc-signer" -> "siga"

  "pdf-converter" -> "apolo"

  "tnu-signer" -> "tnu"
  "blucservice" -> "LCR"

  subgraph cluster_cliente{
